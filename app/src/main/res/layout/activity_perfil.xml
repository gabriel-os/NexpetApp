<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/ic_fundoapp">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="0dp"
        android:background="@layout/btn_redondo"
        app:srcCompat="@color/cardview_shadow_end_color" />

    <TextView
        android:id="@+id/txtEmail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/erro"
        android:textColor="@color/text"
        android:textSize="19sp"
        android:layout_below="@+id/textView25"
        android:layout_alignLeft="@+id/txtSexo"
        android:layout_alignStart="@+id/txtSexo"
        android:background="@color/menuBackground"
        android:textAlignment="center"/>

    <TextView
        android:text="@string/nomedois"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView24"
        android:textColor="@color/text"
        android:background="@color/servBackground"
        android:textAlignment="center"
        style="@android:style/TextAppearance.DeviceDefault.Medium"
        android:textStyle="bold"
        android:layout_below="@+id/imageView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="20dp" />

    <TextView
        android:id="@+id/txtNome"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/erro"
        android:textColor="@color/text"
        android:background="@color/menuBackground"
        android:textAlignment="center"
        android:textSize="19sp"
        android:layout_below="@+id/textView24"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="@string/enderecodois"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView29"
        android:textColor="@color/text"
        style="@android:style/TextAppearance.DeviceDefault.Medium"
        android:layout_below="@+id/txtCelular"
        android:background="@color/servBackground"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textAlignment="center"
        android:textStyle="bold"
        android:layout_marginTop="20dp"/>

    <TextView
        android:id="@+id/txtEndereco"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/erro"
        android:textColor="@color/text"
        android:textSize="19sp"
        android:layout_below="@+id/textView29"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="@color/menuBackground"
        android:textAlignment="center"
        />

    <TextView
        android:text="@string/emaildois"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView25"
        android:textColor="@color/text"
        android:background="@color/servBackground"
        style="@android:style/TextAppearance.DeviceDefault.Medium"
        android:textStyle="bold"
        android:layout_below="@+id/txtNome"
        android:layout_centerHorizontal="true"
        android:textAlignment="center"
        android:layout_marginTop="20dp"/>

    <TextView
        android:text="@string/sexo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView26"
        android:textColor="@color/text"
        style="@android:style/TextAppearance.DeviceDefault.Medium"
        android:textStyle="bold"
        android:background="@color/servBackground"
        android:layout_below="@+id/txtEmail"
        android:layout_centerHorizontal="true"
        android:textAlignment="center"
        android:layout_marginTop="20dp"/>

    <TextView
        android:id="@+id/txtSexo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/erro"
        android:textColor="@color/text"
        android:textSize="19sp"
        android:background="@color/menuBackground"
        android:layout_below="@+id/textView26"
        android:layout_centerHorizontal="true"
        android:textAlignment="center"/>

    <TextView
        android:text="@string/telefonedois"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView27"
        android:textColor="@color/text"
        style="@android:style/TextAppearance.DeviceDefault.Medium"
        android:textStyle="bold"
        android:background="@color/servBackground"
        android:layout_below="@+id/txtSexo"
        android:layout_centerHorizontal="true"
        android:textAlignment="center"
        android:layout_marginTop="20dp"/>

    <TextView
        android:id="@+id/txtTelefone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/erro"
        android:textColor="@color/text"
        android:textSize="19sp"
        android:background="@color/menuBackground"
        android:layout_below="@+id/textView27"
        android:layout_alignLeft="@+id/txtSexo"
        android:layout_alignStart="@+id/txtSexo"
        android:textAlignment="center"/>

    <TextView
        android:text="@string/celulardois"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView28"
        android:textColor="@color/text"
        style="@android:style/TextAppearance.DeviceDefault.Medium"
        android:textStyle="bold"
        android:background="@color/servBackground"
        android:layout_below="@+id/txtTelefone"
        android:layout_centerHorizontal="true"
        android:textAlignment="center"
        android:layout_marginTop="20dp"/>

    <TextView
        android:id="@+id/txtCelular"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/erro"
        android:textColor="@color/text"
        android:textSize="19sp"
        android:background="@color/menuBackground"
        android:layout_below="@+id/textView28"
        android:layout_alignLeft="@+id/txtTelefone"
        android:layout_alignStart="@+id/txtTelefone"
        android:textAlignment="center"/>


</RelativeLayout>